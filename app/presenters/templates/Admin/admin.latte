{block content}
<div class="table">
  <table>
    <tr>
      <th>Id</th>
      <th>Username</th>
      <th>Role</th>
      <th>Email</th>
      <th></th>
     </tr>
    {foreach $users as $u}
      <tr>
        <td>{$u->userId}</td>
        <td>{$u->username}</td>
        <td>{$u->role}</td>
        <td>{$u->email}</td>
        <td><a n:href="changeRole $u->userId">Change role</a></td>
      </tr>
    {/foreach}
  </table>
</div>
